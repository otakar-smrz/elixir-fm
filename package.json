  "bugs": {
    "url": "https://github.com/tomas-hartman/elixir-fm/issues"
  },
  "homepage": "https://github.com/tomas-hartman/elixir-fm#readme",
  "dependencies": {
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.5"
  }
}
